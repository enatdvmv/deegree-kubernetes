<sld:StyledLayerDescriptor version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld ../../appschemas/ogc/sld/1.1.0/StyledLayerDescriptor.xsd" xmlns:sld="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:se="http://www.opengis.net/se" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:app="http://www.deegree.org/app">
  <sld:NamedLayer>
    <se:Name>US.EducationNotElsewhereClassified</se:Name>
    <sld:UserStyle>
      <se:Name>US.EducationNotElsewhereClassified.Default</se:Name>
      <sld:IsDefault>1</sld:IsDefault>
      <se:FeatureTypeStyle version="1.1.0" xmlns:us-govserv="http://inspire.ec.europa.eu/schemas/us-govserv/4.0">
        <se:Description>
          <se:Title>Education not elsewhere classified Default Style</se:Title>
          <se:Abstract>Point geometries are rendered as a triangle with a size of 5 pixel, with a 50% grey (#808080) fill and a black outline.</se:Abstract>
        </se:Description>
        <se:Rule>
          <se:Description>
            <se:Title>Education not elsewhere classified</se:Title>
          </se:Description>
		  <ogc:Filter>
		    <ogc:PropertyIsEqualTo>
		      <ogc:PropertyName type="xs:string">app:type</ogc:PropertyName>
			  <ogc:Literal type="xs:string">http://inspire.ec.europa.eu/codelist/ServiceTypeValue/educationNotElsewhereClassified</ogc:Literal>
		    </ogc:PropertyIsEqualTo>
		  </ogc:Filter>
          <se:PointSymbolizer>
            <se:Geometry>
              <ogc:PropertyName>app:us_geometry</ogc:PropertyName>
            </se:Geometry>
            <se:Graphic>
              <se:Mark>
                <se:WellKnownName>triangle</se:WellKnownName>
                <se:Fill>
                  <se:SvgParameter name="fill">#808080</se:SvgParameter>
                </se:Fill>
                <se:Stroke>
                  <se:SvgParameter name="stroke">#000000</se:SvgParameter>
                  <se:SvgParameter name="stroke-width">1</se:SvgParameter>
                </se:Stroke>
              </se:Mark>
              <se:Size>5</se:Size>
            </se:Graphic>
          </se:PointSymbolizer>
        </se:Rule>
      </se:FeatureTypeStyle>
    </sld:UserStyle>
  </sld:NamedLayer>
</sld:StyledLayerDescriptor>
